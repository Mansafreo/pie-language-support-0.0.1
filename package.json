{
  "name": "pie-language-support",
  "displayName": "PIE Language Support",
  "description": "Syntax highlighting for PIE programming language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "pie",
        "aliases": ["PIE", "pie"],
        "extensions": [".pie"],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/pie-light.svg",
          "dark": "./icons/pie-dark.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "pie",
        "scopeName": "source.pie",
        "path": "./syntaxes/pie.tmLanguage.json"
      }
    ]
  }
}